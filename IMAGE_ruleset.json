         "Column name" : "farm place",
         "Required" : "mandatory"
      },
      {
         "Column name" : "reproduction place",
         "Required" : "optional",
         "Multiple" : "no",
         "Type" : "text",
         "Example" : "Germany"
      },
      {
         "Required" : "optional",
         "Column name" : "photo",
         "Type" : "uri",
         "Multiple" : "yes"
      }
