         "Valid units" : "\"YYYY-MM-DD, YYYY-MM, YYYY\"",
         "Example" : "1990-07-17",
         "Type" : "date",
         "Multiple" : "no"
      },
      {
         "Column name" : "birth status",
         "Required" : "optional",
         "Example" : "normal",
         "Type" : "text",
         "Multiple" : "no",
         "Description" : "\"The status of the birth, could be premature birth for 2 weeks, normal\""
      },
      {
         "Valid ontology terms" : "\"\"\"PATO:0000384\"\",\"\"PATO:0000383\"\"\"",
         "Comment" : "could split into two columns",
         "Required" : "mandatory",
         "Multiple" : "no",
         "Example" : "male (PATO:0000384)",
         "Type" : "limited values",
         "Valid values" : "\"\"\"male\"\",\"\"female\"\"\"",
         "Column name" : "sex",
         "Valid ontology libraries" : "PATO"
      },
      {
         "Type" : "date",
         "Example" : "2014-01-30",
         "Multiple" : "no",
         "Valid units" : "\"YYYY-MM-DD, YYYY-MM, YYYY\"",
         "Required" : "recommended",
         "Column name" : "submission date"
      },
      {
         "Valid units" : "decimal degrees",
         "Required" : "recommended",
         "Column name" : "farm latitude",
         "Example" : "47.86591",
         "Type" : "double",
         "Multiple" : "no"
      },
      {
         "Required" : "recommended",
         "Valid units" : "decimal degrees",
         "Column name" : "farm longitude",
         "Multiple" : "no",
         "Type" : "double",
         "Example" : "12.19115"
      },
      {
         "Example" : "Berlin Germany",
