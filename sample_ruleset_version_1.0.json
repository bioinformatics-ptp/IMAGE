{
   "rule_groups" : [
      {
         "name" : "standard",
         "rules" : [
            {
               "Allow Multiple" : "no",
               "Required" : "mandatory",
               "Description" : "The BioSample id of the sample, automatically generated when sample is registered with IMAGE",
               "Name" : "BioSample id",
               "Type" : "limited value",
               "comments" : "The BioSample ID field will be completed upon submission to BioSamples, when the unqiue identifier will be assigned and returned",
               "Valid values" : [
                  "SAMEAXXXXXXX"
               ],
               "Example" : "SAMEA4450075"
            },
            {
               "Valid values" : [
                  "IMAGE"
               ],
               "Example" : "IMAGE",
               "Type" : "text",
               "Allow Multiple" : "no",
               "Name" : "Project",
               "Required" : "mandatory",
               "Description" : "All samples are stated to be part of the IMAGE project, helps identify them in public databases"
            },
            {
               "Description" : "A descriptive title for the submission",
               "Required" : "mandatory",
               "Allow Multiple" : "no",
               "Type" : "text",
               "Name" : "Submission title",
               "Example" : "German National Sperm Bank"
            },
            {
               "Description" : "A brief description of the submission",
               "Type" : "text",
               "Allow Multiple" : "no",
               "Name" : "Submission description",
               "Required" : "recommended",
               "Example" : "A collection of cattle samples from local breeding stocks"
            },
            {
               "comments" : "the sample could be either animal or specimen",
               "Example" : "Pig sperm sample from 3 year old duroc",
               "Name" : "Sample description",
               "Allow Multiple" : "no",
               "Type" : "text",
               "Required" : "optional",
               "Description" : "A brief description of the sample"
            },
            {
               "Valid terms" : [
                  {
                     "ontology_name" : "OBI",
                     "term_iri" : "http://purl.obolibrary.org/obo/OBI_0100026",
                     "term" : "OBI_0100026",
                     "allow_descendants" : 0,
                     "include_root" : 1
                  },
                  {
                     "include_root" : 1,
                     "ontology_name" : "OBI",
                     "allow_descendants" : 0,
                     "term_iri" : "http://purl.obolibrary.org/obo/OBI_0001479",
                     "term" : "OBI_0001479"
                  }
               ],
               "Required" : "mandatory",
               "Allow Multiple" : "no",
               "Description" : "type of the sample, either animal(organism) or sample (specimen from organism)",
               "Example" : "Specimen from Organism",
               "Valid values" : [
                  "Organism",
                  "Specimen from organism"
               ],
               "Type" : "ontology_id",
               "Name" : "Material"
            },
            {
               "Name" : "Person last name",
               "Allow Multiple" : "yes",
               "Type" : "text",
               "Required" : "mandatory",
               "Example" : "Harrison",
               "Description" : "the last name of the submitter or contact for these samples, multiple persons can be provided"
            },
            {
               "Description" : "the initial of the submitter or contact for these samples, multiple persons can be provided",
               "Required" : "optional",
               "Allow Multiple" : "yes",
               "Name" : "Person initial",
               "Type" : "text",
               "Example" : "W",
               "comments" : "One person can have multiple initials"
            },
            {
               "Type" : "text",
               "Allow Multiple" : "yes",
               "Name" : "Person first name",
               "Required" : "recommended",
               "Example" : "Peter",
               "Description" : "the first name of the submitter or contact for these samples, multiple persons can be provided"
            },
            {
               "Description" : "email of contact person",
               "Type" : "uri_value",
               "Allow Multiple" : "yes",
               "Name" : "Person email",
               "Required" : "mandatory",
               "Example" : "mailto:peter@ebi.ac.uk"
            },
            {
               "Type" : "text",
               "Allow Multiple" : "yes",
               "Name" : "Person affiliation",
               "Required" : "mandatory",
               "comments" : "One person can have multiple affilations",
               "Example" : "EMBL-EBI",
               "Description" : "affiliation of person"
            },
            {
               "Example" : "Submitter",
               "Valid terms" : [
                  {
                     "ontology_name" : "EFO",
                     "allow_descendants" : 1,
                     "term_iri" : "http://purl.obolibrary.org/obo/EFO_0002012",
                     "term" : "EFO_0002012",
                     "include_root" : 0
                  }
               ],
               "Required" : "mandatory",
               "Type" : "text",
               "Allow Multiple" : "yes",
               "Name" : "Person role",
               "Description" : "the role (type of the contribution) made by the person"
            },
            {
               "Example" : "EMBL-EBI",
               "Allow Multiple" : "yes",
               "Type" : "text",
               "Name" : "Organisation name",
               "Required" : "mandatory",
               "Description" : "Organisation name, multiple can be supplied"
            },
            {
               "Description" : "The address of the organisation, multiple can be supplied",
               "Required" : "recommended",
               "Allow Multiple" : "yes",
               "Type" : "text",
               "Name" : "Organisation address",
               "Example" : "European Bioinformatics Institute, Wellcome Trust Genome Campus, CB10 1SD, UK"
            },
            {
               "Allow Multiple" : "yes",
               "Type" : "uri_value",
               "Name" : "Organisation uri",
               "Required" : "optional",
               "Example" : "www.ebi.ac.uk",
               "Description" : "the websites of orgnisation, multiple can be supplied"
            },
            {
               "Description" : "Country in which organisation resides, the controlled vocabulary for countries is from INSDC http://www.insdc.org/documents/country-qualifier-vocabulary",
               "Example" : "United Kingdom",
               "Valid values" : [
                  "the controlled vocabulary for countries is from INSDC http://www.insdc.org/documents/country-qualifier-vocabulary"
               ],
               "Required" : "recommended",
               "Type" : "text",
               "Allow Multiple" : "yes",
               "Name" : "Organisation country"
            },
            {
               "Example" : "Submitter",
               "Valid terms" : [
                  {
                     "ontology_name" : "EFO",
                     "term_iri" : "http://purl.obolibrary.org/obo/EFO_0002012",
                     "term" : "EFO_0002012",
                     "allow_descendants" : 1,
                     "include_root" : 0
                  }
               ],
               "Required" : "mandatory",
               "Name" : "Organisation role",
               "Allow Multiple" : "yes",
               "Type" : "ontology_id",
               "Description" : "the role of the organisation, e.g. biobank, funder"
            },
            {
               "Allow Multiple" : "no",
               "Type" : "doi",
               "Name" : "Publication DOI",
               "Required" : "optional",
               "Example" : "doi://10.1139/o09-005",
               "Description" : "Publication associated with sample, supplied as the digital object identifier which can unambiguously point to the publication"
            },
            {
               "Description" : "the name of the genebank source type",
               "Example" : "CyroWEB DE, French system",
               "Required" : "mandatory",
               "Name" : "Data source name",
               "Allow Multiple" : "no",
               "Type" : "text"
            },
            {
               "Description" : "the version of the genebanks storage software",
               "Example" : "2017-04, version 1.1",
               "Required" : "recommended",
               "Type" : "text",
               "Allow Multiple" : "no",
               "Name" : "Data source version"
            }
         ]
      },
      {
         "name" : "organism",
         "condition" : {
            "attribute_value_match" : {
               "Material" : "Organism"
            }
         },
         "rules" : [
            {
               "Example" : "OAR_RI_AF1, ANIMAL:::ID:::132713",
               "Allow Multiple" : "no",
               "Name" : "Animal name",
               "Type" : "text",
               "Required" : "mandatory",
               "Description" : "the name of the animal used in the original data source"
            },
            {
               "Example" : "11",
               "Required" : "recommended",
               "Type" : "limited values",
               "Allow Multiple" : "no",
               "Name" : "Alternative animal ID",
               "Description" : "the animal id used in the original data source"
            },
            {
               "Description" : "Father and/or Mother's Alternative animal ID whose record must also be present as part of the subission or BioSamples ID if sample has been previously submitted",
               "Allow Multiple" : "max 2",
               "Name" : "Derived from",
               "Type" : "BioSample ID or Alternative animal ID",
               "Required" : "optional",
               "Example" : "SAMEA4450075 or 11"
            },
            {
               "Name" : "Species",
               "Allow Multiple" : "no",
               "Type" : "ontology_id",
               "Required" : "mandatory",
               "Valid terms" : [
                  {
                     "term_iri" : "http://purl.obolibrary.org/obo/NCBITaxon_1",
                     "term" : "NCBITaxon_1",
                     "allow_descendants" : 1,
                     "ontology_name" : "NCBITaxon",
                     "include_root" : 0
                  }
               ],
               "Example" : "Sus scrofa",
               "Description" : "the species of the animal"
            },
            {
               "Example" : "Bunte Bentheimer",
               "Required" : "mandatory",
               "Allow Multiple" : "no",
               "Type" : "text",
               "Name" : "Supplied breed",
               "Description" : "the breed information contained in the original gene bank, could be a local language or breed variation"
            },
            {
               "Required" : "mandatory",
               "Name" : "EFABIS Breed country",
               "Allow Multiple" : "no",
               "Type" : "text",
               "Example" : "Germany",
               "Description" : "the country where the breed locates, the country name should be in the INSDC list"
            },
            {
               "Required" : "recommended",
               "Allow Multiple" : "no",
               "Type" : "ontology_id",
               "Name" : "Mapped breed",
               "Example" : "LBO_0000347",
               "Valid terms" : [
                  {
                     "include_root" : 0,
                     "term_iri" : "http://www.ebi.ac.uk/ols/ontologies/lbo/terms?iri=http%3A%2F%2Fpurl.obolibrary.org%2Fobo%2FLBO_0000000",
                     "term" : "LBO_0000000",
                     "allow_descendants" : 1,
                     "ontology_name" : "LBO"
                  }
               ],
               "Description" : "If possible breed will be mapped to a standardised breed ontology, both supplied and mapped breed will be available to end users"
            },
            {
               "Required" : "mandatory",
               "Allow Multiple" : "no",
               "Name" : "Sex",
               "Type" : "ontology_id",
               "Example" : "PATO:0000384 (male)",
               "Valid terms" : [
                  {
                     "ontology_name" : "PATO",
                     "term_iri" : "http://purl.obolibrary.org/obo/PATO_0000384",
                     "allow_descendants" : 0,
                     "term" : "PATO_0000384",
                     "include_root" : 1
                  },
                  {
                     "allow_descendants" : 0,
                     "term_iri" : "http://purl.obolibrary.org/obo/PATO_0000383",
                     "term" : "PATO_0000383",
                     "ontology_name" : "PATO",
                     "include_root" : 1
                  }
               ],
               "Description" : "the sex of the animal"
            },
            {
               "Example" : "Germany",
               "Required" : "recommended",
               "Allow Multiple" : "no",
               "Type" : "text",
               "Name" : "Birth location",
               "Description" : "name of the place where the animal was given birth"
            },
            {
               "Example" : "47.86591",
               "Valid units" : [
                  "decimal degrees"
               ],
               "Type" : "number",
               "Allow Multiple" : "no",
               "Name" : "Birth location longitude",
               "Required" : "recommended",
               "Description" : "the longitude of the place where the animal was given birth in decimal degrees"
            },
            {
               "Description" : "the latitude of the place where the animal was given birth in decimal degrees",
               "Example" : "12.19115",
               "Required" : "recommended",
               "Allow Multiple" : "no",
               "Valid units" : [
                  "decimal degrees"
               ],
               "Type" : "number",
               "Name" : "Birth location latitude"
            }
         ]
      },
      {
         "rules" : [
            {
               "Example" : "Siems_0722_393449",
               "Required" : "mandatory",
               "Name" : "Alternative id",
               "Allow Multiple" : "no",
               "Type" : "limited value",
               "Description" : "the sample id used in the original data source, unique and identifiable within the submitting gene bank"
            },
            {
               "Description" : "The Alternative animal ID (whose record must also be present as part of the subission) or BioSamples ID (if sample has been previously submitted) of the animal from which sample specimen was collected",
               "Type" : "text",
               "Allow Multiple" : "no",
               "Name" : "Derived from",
               "Required" : "mandatory",
               "Example" : "SAMEA4450079"
            },
            {
               "Required" : "mandatory",
               "Type" : "text",
               "Allow Multiple" : "no",
               "Name" : "Sample name",
               "Example" : "OAR_RI_AF1_AA",
               "Description" : "the name of the sample given by the submitter"
            },
            {
               "Description" : "A URL link to the protocol used to collect this sample",
               "Type" : "uri",
               "Allow Multiple" : "no",
               "Name" : "Specimen collection protocol",
               "Required" : "recommended",
               "Example" : "ftp://ftp.faang.ebi.ac.uk/ftp/protocols/samples/WUR_SOP_animal_sampling_20160405.pdf"
            },
            {
               "Description" : "Date when the sample was collected",
               "Example" : "17/01/1992",
               "Required" : "mandatory",
               "Type" : "date",
               "Valid units" : [
                  "YYYY-MM-DD",
                  "YYYY-MM",
                  "YYYY"
               ],
               "Allow Multiple" : "no",
               "Name" : "Collection date"
            },
            {
               "Valid units" : [
                  "decimal degrees"
               ],
               "Allow Multiple" : "no",
               "Name" : "Collection place latitude",
               "Type" : "number",
               "Required" : "recommended",
               "Example" : "47.86591",
               "Description" : "latitude of the place where the sample was collected"
            },
            {
               "Valid units" : [
                  "decimal degrees"
               ],
               "Allow Multiple" : "no",
               "Type" : "number",
               "Name" : "Collection place longitude",
               "Required" : "recommended",
               "Example" : "12.19115",
               "Description" : "longitude of the place where the sample was collected"
            },
            {
               "Description" : "name of the place where the sample was collected",
               "Example" : "Germany",
               "Required" : "mandatory",
               "Allow Multiple" : "no",
               "Type" : "text",
               "Name" : "Collection place"
            },
            {
               "Required" : "mandatory",
               "Allow Multiple" : "no",
               "Valid terms" : [
                  {
                     "include_root" : 1,
                     "ontology_name" : "UBERON",
                     "term_iri" : "http://purl.obolibrary.org/obo/UBERON_0001968",
                     "allow_descendants" : 0,
                     "term" : "UBERON_0001968"
                  },
                  {
                     "term_iri" : "http://purl.obolibrary.org/obo/UBERON_0000922",
                     "term" : "UBERON_0000922",
                     "allow_descendants" : 0,
                     "ontology_name" : "UBERON",
                     "include_root" : 1
                  }
               ],
               "Name" : "Organism part",
               "Type" : "ontology_id",
               "Example" : "UBERON_0001968 (semen)",
               "Valid values" : [
                  "semen",
                  "embryo"
               ],
               "Description" : "Ontology for part of organism specimen from, currently either semen or embryo. If any other organism part is collected, please contact us"
            },
            {
               "Description" : "developmental stage of the animal from which the specimen is collected",
               "Valid terms" : [
                  {
                     "include_root" : 0,
                     "ontology_name" : "EFO",
                     "allow_descendants" : 1,
                     "term_iri" : "http://purl.obolibrary.org/obo/EFO_0000399",
                     "term" : "EFO_0000399"
                  }
               ],
               "Example" : "EFO_0001272 (Adult)",
               "Allow Multiple" : "no",
               "Name" : "Developmental stage",
               "Type" : "ontology_id",
               "Required" : "recommended"
            },
            {
               "Description" : "Animal age at the point of collection.",
               "Allow Multiple" : "no",
               "Type" : "number",
               "Name" : "Animal age at collection",
               "Required" : "optional",
               "Example" : "3 years"
            },
            {
               "Description" : "Either an e-mail address to contact about availability or a link to a web page giving information on who to contact. E-mail addresses should be prefixed with 'mailto:', e.g. 'mailto:samples@example.ac.uk'. In either case, long term support of the web page or e-mail address is necessary. Group e-mail addresses are preferable to indiviudal accounts.",
               "Required" : "recommended",
               "Name" : "Availability",
               "Allow Multiple" : "no",
               "Type" : "uri_value",
               "Example" : "mailto:vdc-internal@ebi.ac.uk"
            }
         ],
         "condition" : {
            "attribute_value_match" : {
               "Material" : "Specimen from Organism"
            }
         },
         "name" : "specimen from organism"
      }
   ],
   "further_details_iri" : "https://github.com/bioinformatics-ptp/IMAGE-metadata/blob/master/README.md",
   "description" : "Validation rules for the IMAGE project.",
   "name" : "IMAGE sample metadata rules"
}

